    "@fastify/static": "7.0.4",
    "@formatjs/intl": "2.10.4",
    "@hotwired/turbo": "8.0.4",
    "@nanoweb/jsx": "0.0.19",
    "fastify": "4.28.1",
    "flat": "6.0.1",
    "get-jwks": "9.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "eslint-plugin-promise": "6.4.0",
    "pino-pretty": "11.2.1",
    "prettier": "3.3.2",
