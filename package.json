{
  "name": "velocity-docs",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production node build.js",
    "check-formatting": "prettier --check 'src/**/*.ts{,x}'",
    "check-typing": "tsc --noEmit",
    "dev": "NODE_ENV=development node --env-file=.env build.js | pino-pretty",
    "fix-formatting": "prettier --write 'src/**/*.ts{,x}'",
    "fix-lint": "eslint --fix 'src/**/*.ts{,x}'",
    "lint": "eslint 'src/**/*.ts{,x}'",
    "start": "node --enable-source-maps ./.build/production/index.js"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/static": "7.0.4",
    "@hotwired/turbo": "8.0.4",
    "@nanoweb/jsx": "0.0.19",
    "@tailwindcss/typography": "0.5.15",
    "fastify": "4.28.1",
    "gray-matter": "4.0.3",
    "highlight.js": "11.10.0",
    "marked": "14.1.3",
    "smartypants": "0.2.2"
  },
  "devDependencies": {
    "@types/hotwired__turbo": "8.0.1",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "eslint-plugin-promise": "6.4.0",
    "pino-pretty": "11.2.1",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.5",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3"
  }
}
